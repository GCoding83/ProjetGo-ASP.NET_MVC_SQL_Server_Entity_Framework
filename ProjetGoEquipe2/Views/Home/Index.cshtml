    @{
    ViewBag.Title = "Home Page";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - ProjetGo</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>

    <main role="main" class="container-fluid">
        <div class="col-lg-12">

            <div class="row">

                <div class="content-section">

                    <h1>Bienvenue sur le site de ProjetGo</h1>
                    <!-- Carousel
                ================================================== -->
                    <div id="myCarousel" class="carousel slide" data-ride="carousel">
                        <!-- Indicators -->
                        <ol class="carousel-indicators">
                            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                            <li data-target="#myCarousel" data-slide-to="1"></li>
                            <li data-target="#myCarousel" data-slide-to="2"></li>
                        </ol>
                        <div class="carousel-inner">
                            <div class="item active" style="max-height:150px;">
                                <img src="~/Content/Images/ramasser-roche.jpg" alt="First slide">
                                <div class="container">
                                    <div class="carousel-caption">
                                        <h1>Test</h1>
                                        <p>Test</p>
                                    </div>
                                </div>
                            </div>
                            <div class="item" style="max-height:150px;">
                                <img src="~/Content/Images/vente-garage.jpg" alt="Second slide">
                                <div class="container">
                                    <div class="carousel-caption">
                                        <h1>Test</h1>
                                        <p>Test</p>
                                    </div>
                                </div>
                            </div>
                            <div class="item" style="max-height:150px;">
                                <img src="~/Content/Images/soupe-sans-abris.jpg" alt="Third slide">
                                <div class="container">
                                    <div class="carousel-caption">
                                        <h1>Test</h1>
                                        <p>Test</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <a class="left carousel-control" href="#myCarousel" data-slide="prev"><span class="glyphicon glyphicon-chevron-left"></span></a>
                        <a class="right carousel-control" href="#myCarousel" data-slide="next"><span class="glyphicon glyphicon-chevron-right"></span></a>
                    </div><!-- /.carousel -->
                    <br />
                    <br />
                </div>
            </div>
        </div>

        <div class="row">
            <div class="content-section">
                <div class="col-sm-8">
                    <iframe width="700" height="315" src="https://www.youtube.com/embed/099_WKUBh6A" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>   
                </div>

                <div class="form-group">
                    <div class="table-bordered col-sm-4" >
                        
                        <marquee class="container-fluid" direction="down" style="height:300px; text-align:center">
                            <p><b>Projet en cours : Liste des projets actif</b></p>
                            <br /><br />
                            <p><b>Nous acceptons de nouveau les demandes de projets</b></p>
                            <br /><br />
                            <p><b>Reprise des activités de projetGo suite à la pandémie</b></p>
                            <br /><br />
                            <p><b>Arrêt temporaire des activités de projetGo due à la pandémie</b></p>
                            <br /><br />

                        </marquee>
                    </div>

                </div>
            </div>
        </div>
    </main>

</body>
</html>